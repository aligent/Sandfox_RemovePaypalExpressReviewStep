<config>
    <sections>
        <payment>
            <groups>
                <paypal_payments>
                    <wps_express>
                        <fields>
                            <settings_wps_express>
                                <fields>
                                    <settings_wps_express_advanced>
                                        <fields>
                                            <force_use_customer_name>
                                                <label>Override Paypal Shipping Name with Customer Name (if available)</label>
                                                <config_path>payment/paypal_express/force_use_customer_name</config_path>
                                                <frontend_type>select</frontend_type>
                                                <source_model>adminhtml/system_config_source_yesno</source_model>
                                                <sort_order>96</sort_order>
                                                <show_in_default>1</show_in_default>
                                                <show_in_website>1</show_in_website>
                                                <shared>1</shared>
                                            </force_use_customer_name>
                                            <duplicate_shipping_address_to_billing>
                                                <label>Duplicate Paypal shipping address into Magento billing address</label>
                                                <config_path>payment/paypal_express/use_shipping_as_billing</config_path>
                                                <frontend_type>select</frontend_type>
                                                <source_model>adminhtml/system_config_source_yesno</source_model>
                                                <sort_order>97</sort_order>
                                                <show_in_default>1</show_in_default>
                                                <show_in_website>1</show_in_website>
                                                <shared>1</shared>
                                            </duplicate_shipping_address_to_billing>
                                        </fields>
                                    </settings_wps_express_advanced>
                                </fields>
                            </settings_wps_express>
                        </fields>
                    </wps_express>
                </paypal_payments>
            </groups>
        </payment>
    </sections>
</config>
